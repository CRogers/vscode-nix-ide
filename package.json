{
  "name": "nix-ide",
  "displayName": "Nix IDE",
  "description": "Nix language support with formatting and error report.",
  "version": "0.0.1",
  "publisher": "jnoortheen",
  "icon": "images/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "bugs": {
    "url": "https://github.com/jnoortheen/vscode-nix-ide/issues"
  },
  "homepage": "https://github.com/jnoortheen/vscode-nix-ide",
  "repository": {
    "type": "git",
    "url": "https://github.com/jnoortheen/vscode-nix-ide"
  },
  "main": "dist/Extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nix",
        "aliases": [
          "Nix",
          "nix"
        ],
        "extensions": [
          ".nix"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nix",
        "scopeName": "source.nix",
        "path": "./syntaxes/nix.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "nix",
        "path": "./snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "fable-compiler": "^2.10.1",
    "fable-splitter": "^2.2.1",
    "js-yaml": "^3.14.0",
    "standard-version": "^8.0.1"
  },
  "scripts": {
    "prebuild": "js-yaml syntaxes/nix.YAML-tmLanguage > syntaxes/nix.tmLanguage.json",
    "build": "fable-splitter --config splitter.config.js",
    "watch": "yarn build --watch",
    "release": "yarn build && standard-version"
  }
}
